{
    "configurations": [{
        "name": "Win32",
        "includePath": [
            "${workspaceFolder}/**",
            "${env:llvm}lib/gcc/x86_64-w64-mingw32/8.1.0/include/**",
            "${env:llvm}/x86_64-w64-mingw32/include/**"
        ],
        "defines": [
            "_DEBUG",
            "UNICODE",
            "_UNICODE"
        ],
        "browse": {
            "path": [
                "${env:llvm}/x86_64-w64-mingw32/include",
                "${env:llvm}/lib/gcc/x86_64-w64-mingw32/8.1.0/include",
                "${env:llvm}/lib/gcc/x86_64-w64-mingw32/8.1.0/include-fixed",
                "${env:llvm}/include/*",
                "${workspaceRoot}"
            ],
            "limitSymbolsToIncludedHeaders": true,
            "databaseFilename": ""
        },
        "compilerPath": "D:/LLVM/bin/clang++.exe",
        "cStandard": "c11",
        "cppStandard": "c++17",
        "intelliSenseMode": "clang-x64"
    }],
    "version": 4
}